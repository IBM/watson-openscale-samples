                "# 6. Drift V2 monitoring <a name=\"drift_v2\"></a>"
            ]
        },
        {
            "cell_type": "markdown",
            "metadata": {
                "id": "fdeaed67-e74c-4d7f-83ca-6f1d258606ff"
            },
            "source": [
                "### Enable the drift v2 monitor\n",
                "\n",
                "In the following code cell, type a path to the drift v2 configuration tar ball."
            ]
        },
        {
            "cell_type": "code",
            "execution_count": 121,
            "metadata": {
                "id": "c370ae33-f9e2-4287-bba8-8c2b841bdcfc"
            },
            "outputs": [],
            "source": [
                "monitor_instances = wos_client.monitor_instances.list().result.monitor_instances\n",
                "for monitor_instance in monitor_instances:\n",
                "    monitor_def_id=monitor_instance.entity.monitor_definition_id\n",
                "    if monitor_def_id == \"drift_v2\" and monitor_instance.entity.target.target_id == subscription_id:\n",
                "        wos_client.monitor_instances.delete(monitor_instance.metadata.id)\n",
                "        print('Deleted existing drift_v2 monitor instance with id: ', monitor_instance.metadata.id)"
            ]
        },
        {
            "cell_type": "code",
            "execution_count": 123,
            "metadata": {
                "id": "88175c18-4351-457a-82b7-300438619cf4"
            },
            "outputs": [
                {
                    "data": {
                        "text/plain": [
                            "{}"
                        ]
                    },
                    "execution_count": 123,
                    "metadata": {},
                    "output_type": "execute_result"
                }
            ],
            "source": [
                "wos_client.monitor_instances.upload_drift_v2_archive(\n",
                "    archive_path=\"/path/to/dir/containing/drift_v2_archive.tar.gz\",\n",
                "    data_mart_id=data_mart_id,\n",
                "    subscription_id=subscription_id\n",
                ").result"
            ]
        },
