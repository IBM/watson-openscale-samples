            "metadata": {
                "id": "e165900d-ef38-4cdb-90d2-3a1f21a6850a"
            },
            "outputs": [],
            "source": [
                "# Service Provider\n",
                "\n",
                "SERVICE_PROVIDER_NAME = \"\"\n",
                "SERVICE_PROVIDER_DESCRIPTION = \"\"\n",
                "\n",
                "# Subscription\n",
                "\n",
