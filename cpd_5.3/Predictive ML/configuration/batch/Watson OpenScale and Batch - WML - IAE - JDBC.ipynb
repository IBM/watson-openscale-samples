                "    description=IAE_SPARK_DESCRIPTION,\n",
                "    type=\"spark\",\n",
                "    credentials={\n",
                "        \"username\": IBM_CPD_USERNAME,\n",
                "        \"apikey\": IBM_CPD_APIKEY\n",
                "    },\n",
                "    connection={\n",
                "        \"display_name\": IAE_SPARK_DISPLAY_NAME,\n",
                "        \"endpoint\": IAE_SPARK_JOBS_ENDPOINT,\n",
                "        \"volume\": IBM_CPD_VOLUME,\n",
